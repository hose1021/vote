{
    "name": "hose1021/vote",
    "description": "vote",
    "keywords": [
        "Michael Hose",
        "vote"
    ],
    "homepage": "https://github.com/hose1021/vote",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Hose",
            "email": "hose1021@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.13",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Michael Hose\\Vote\\": "src",
            "Michael Hose\\Vote\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Michael Hose\\Vote\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Michael Hose\\Vote\\VoteServiceProvider"
            ],
            "aliases": {
                "Vote": "Michael Hose\\Vote\\VoteFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
